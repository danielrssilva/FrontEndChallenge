  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/(components|helpers)/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/**/*.styles.ts",
      "!src/**/*.types.ts",
      "!src/**/index.ts",
      "!src/services/*.ts",
      "!<rootDir>/src/index.tsx",
      "!src/pages/*/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.179",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-typist": "^2.0.3",
    "@types/styled-components": "^5.1.21",
    "cypress": "^9.5.4",
    "prettier": "2.5.1"
  }
